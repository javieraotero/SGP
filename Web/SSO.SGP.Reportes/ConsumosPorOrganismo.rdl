  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>CPP_DesaDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Desde">
            <Value>=Parameters!Desde.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Hasta">
            <Value>=Parameters!Hasta.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Ministerio">
            <Value>=Parameters!Ministerio.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT A.Codigo, CONVERT(VARCHAR(50), A.Nombre) AS Articulo, SUM(AP.CantidadEntregada) Total, A.UltimoCosto, O.Descripcion, SUM(AP.CantidadEntregada) * A.UltimoCosto TotalPesos, CASE WHEN @Ministerio = 1 THEN 'Ministerio Público' ELSE '' END Ministerio
FROM ArticulosDePedidoDeSuministros AP
INNER JOIN ArticulosDeSuministros A ON AP.Articulo = A.Id 
--INNER JOIN tblMovimientosDeSuministros M ON M.Articulo = A.Id
--INNER JOIN tblTiposDeMovimientosDeSuministros T ON T.Id = M.TipoDeMovimiento
LEFT JOIN PedidosdeSuministros P ON P.Id = AP.Pedido
LEFT JOIN OrganismosRef O ON P.Organismo = O.Id
WHERE O.Descripcion IS NOT NULL
AND ((@Desde IS NULL) OR (@Desde IS NOT NULL AND P.FechaDePedido &gt;= @Desde))
AND ((@Hasta IS NULL) OR (@Hasta IS NOT NULL AND P.FechaDePedido &lt;= @Hasta))
AND A.Nombre NOT LIKE '%Javier%'
AND (
(@Ministerio = 1 AND Dependede in (328,329)) OR (@Ministerio = 0 AND (DependeDe not in (328, 329) OR DependeDe IS NULL)))
GROUP BY A.Codigo, A.Nombre, O.Descripcion, A.UltimoCosto</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Codigo">
          <DataField>Codigo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Articulo">
          <DataField>Articulo</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UltimoCosto">
          <DataField>UltimoCosto</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Descripcion">
          <DataField>Descripcion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalPesos">
          <DataField>TotalPesos</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Ministerio">
          <DataField>Ministerio</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
