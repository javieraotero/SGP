      </Fields>
      <Query>
        <DataSourceName>CPP_DesaDataSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>select convert(varchar(10), A.Codigo), convert(varchar(60), A.Nombre) as Articulo, sum(AP.CantidadEntregada) AS Total
from ArticulosDeSuministros A
inner join ArticulosDePedidoDeSuministros AP on A.Id = AP.Articulo
inner join PedidosDeSuministros P on AP.Pedido = P.Id
inner join OrganismosRef O on O.id = P.Organismo
where P.FechaDePedido &gt;= @Desde and p.FechaDePedido &lt;= @Hasta and a.nombre not like '%javier%' 
and DependeDe in (328, 329)
group by a.codigo, a.Nombre
order by a.Nombre</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
