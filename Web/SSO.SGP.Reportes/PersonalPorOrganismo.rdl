  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>CPP_DesaDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Organismo">
            <Value>=Parameters!Organismo.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT O.Id, o.Descripcion, LTRIM(RTRIM(p.Apellidos)) + ',' + p.Nombres AS Agente, c.Descripcion AS Categoria, FechaDeAlta, N.FechaUltimoAscenso, cI.Descripcion as Circunscripcion, c.Id AS CargoID, Legajo, FechaFinContrato, N.SituacionRevista,  C.Grupo, C.Orden
FROM dbo.OrganismosRef O
INNER JOIN dbo.Nombramientos N ON o.Id = N.Organismo
INNER JOIN dbo.Agentes A ON A.Id = N.Agente
INNER JOIN dbo.CargosRef C ON N.Cargo = C.Id
INNER JOIN dbo.Personas P ON P.Id = A.Persona
LEFT JOIN dbo.CircunscripcionesRef ci ON cI.Id = o.Circunscripcion
WHERE N.FechaDeBaja IS NULL
AND N.FechaEliminacion IS NULL AND O.Id = @Organismo
ORDER BY grupo, orden</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
