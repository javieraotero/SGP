      <Query>
        <DataSourceName>CPPSource</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT P.Id, FechaDePedido, O.Descripcion AS Organismo, CantidadEntregada, AR.Nombre AS Articulo, CantidadPedida 
FROM dbo.PedidosDeSuministros P 
INNER JOIN dbo.ArticulosDePedidoDeSuministros A ON p.Id = a.Pedido
INNER JOIN dbo.OrganismosRef O ON O.Id = p.Organismo
INNER JOIN dbo.ArticulosDeSuministros AR ON AR.id = A.Articulo
WHERE P.Id = @Pedido</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@Pedido">
            <Value>=Parameters!Pedido.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
