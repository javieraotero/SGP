    <DataSource Name="SQL_Testing">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=10.22.231.102\sql2012_desa;Initial Catalog=CPP_Test</ConnectString>
        <Prompt>Especifique un nombre de usuario y una contraseña para el origen de datos SQL_Testing:</Prompt>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>a9f7fc20-d7d7-4c73-a34d-9eaa121f8aba</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>SQL_Testing</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Desde">
            <Value>=Parameters!Desde.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Hasta">
            <Value>=Parameters!Hasta.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Categoria">
            <Value>=Parameters!Categoria.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT O.Id AS ID, o.Descripcion, LTRIM(RTRIM(p.Apellidos)) + ',' + p.Nombres AS Agente, c.Descripcion AS Categoria, FechaDeAlta AS FechaDeAlta, N.FechaUltimoAscenso, cI.Descripcion as Circunscripcion, c.Id AS CargoID, Legajo, FechaFinContrato, FechaFinSustitucion
FROM dbo.OrganismosRef O
INNER JOIN dbo.Nombramientos N ON o.Id = N.Organismo
INNER JOIN dbo.Agentes A ON A.Id = N.Agente
INNER JOIN dbo.CargosRef C ON N.Cargo = C.Id
INNER JOIN dbo.Personas P ON P.Id = A.Persona
LEFT JOIN dbo.CircunscripcionesRef ci ON cI.Id = o.Circunscripcion
WHERE N.FechaUltimoAscenso &gt;= @Desde AND N.FechaUltimoAscenso &lt;= @Hasta
AND (@Categoria &lt;= 0 OR (@Categoria &gt; 0  AND @Categoria = C.Id) )
AND N.FechaDeAlta &lt;&gt; N.FechaUltimoAscenso</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Descripcion">
          <DataField>Descripcion</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Agente">
          <DataField>Agente</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
