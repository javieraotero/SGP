@{
    Layout = null;
}
@model SSO.SGP.EntidadesDeNegocio.LicenciasAgente

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Aprobada</title>
</head>
<body>
    <div>
        <p>
            La Licencia del agente @Model.AgenteRRHHs.Personas.Apellidos del @Model.FechaDesde.ToShortDateString() al @Model.FechaHasta.Value.ToShortDateString() ha sido desaprobada.
        </p>
        <p style="text-align:center">
            <img src="~/assets/img/sistemasfirma.png"/>
        </p>
    </div>
</body>
</html>
